<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>Inventory</display-name>
  <welcome-file-list>
    <welcome-file>home</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>equipServlet</servlet-name>
    <servlet-class>org.orange.web.servlet.EquipmentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>equipServlet</servlet-name>
    <url-pattern>/equipments</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>fPasswordServlet</servlet-name>
    <servlet-class>org.orange.web.servlet.ForgetPasswordServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>fPasswordServlet</servlet-name>
    <url-pattern>/forget-password</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>showEquipmentServlet</servlet-name>
    <servlet-class>org.orange.web.servlet.ShowEquipmentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>showEquipmentServlet</servlet-name>
    <url-pattern>/equipments/show</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>homeServlet</servlet-name>
    <servlet-class>org.orange.web.servlet.HomeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>homeServlet</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>addEquipServlet</servlet-name>
    <servlet-class>org.orange.web.servlet.AddEquipmentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>addEquipServlet</servlet-name>
    <url-pattern>/add-equipment</url-pattern>
  </servlet-mapping>
    <servlet>
    <servlet-name>manageLoginServlet</servlet-name>
    <servlet-class>org.orange.web.servlet.ManageLoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>manageLoginServlet</servlet-name>
    <url-pattern>/manage-login</url-pattern>
  </servlet-mapping>
</web-app>